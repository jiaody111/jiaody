.exps-container {
  display: flex;
  flex-direction: row;
  height: auto;
  width: 750rpx;
  align-items: center;
  justify-content: flex-start;
  background-color: white;
  border-bottom: 1px dotted silver;
}

.exp-name {
  color: slategray;
  width: 240rpx;
  height: 150rpx;
  line-height: 120rpx;
  font-weight: bold;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-right: 1px dotted silver;
}

.exp-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 500rpx;
  height: 150rpx;
  overflow: auto;
}

.exp-algo-name {
  color: slategray;
  width: 240rpx;
  font-weight: bold;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;

}

.exp-algo-item {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 500rpx;
  height: auto;
  overflow: auto;
    border-left: 1px dotted silver;
}
