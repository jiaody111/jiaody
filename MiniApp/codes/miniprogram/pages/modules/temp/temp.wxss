}

.Algo-Item {
  height: 80rpx;
  margin-bottom: 15rpx;
  margin-top: 20rpx;
  justify-content: flex-start;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.param-name {
  width: 340rpx;
  margin-left: 30rpx;
  height: 80rpx;
