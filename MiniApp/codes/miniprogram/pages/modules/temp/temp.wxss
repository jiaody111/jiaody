.description-container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  width: 740rpx;
}

.alg-description {
  position: absolute;
  font-size: 34rpx;
  width: 720rpx;
  margin-left: 10rpx;
  display: flex;
  top: 30rpx;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  color: #337ab7;
}

.data-description {
  display: flex;
  flex-direction: row;
